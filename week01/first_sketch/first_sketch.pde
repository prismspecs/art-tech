// the size function determines the window size
// as width, height in pixels
size(500, 500);
background(255, 127, 0);

// the ellipse function draws an ellipse at
// x, y, width, height
ellipse(250, 250, 120, 120);

// give it an outline
stroke(0, 250, 255);
// change the outline thickness in pixels
strokeWeight(5);

// change the inside color
fill(255, 0, 0);

// rectangles are drawn at
// x, y, width, height
rect(250, 250, 100, 100);

// remove the outline
noStroke();

rect(450, 350, 100, 100);
